  "pins": {
    "IN+": {
      "name": "IN+",
      "pin_id": "1",
      "type": "ANALOG",
      "direction": "INPUT",
      "description": "Input current positive pin. Current flows from IN+ to IN- through an internal low-resistance conductor (0.7mΩ typ at 25°C). The conductor generates a magnetic field proportional to input current, measured by an integrated Hall sensor. Maximum continuous current: 80A RMS at 25°C, 44A RMS at 125°C. Input conductor resistance temperature coefficient: 2.1µΩ/°C.",
      "implementation": {
        "connectivity": "Connect to high-side current path. Route with heavy copper traces (4oz recommended) to minimize resistance and improve thermal dissipation. Keep path from IN+ to bypass capacitors as short as possible.",
        "components": "Use appropriate wire gauge or PCB trace width for expected current levels. Consider thermal via farms around input pins for heat dissipation.",
        "precautions": "Do not exceed Safe Operating Area (SOA) current limits based on ambient temperature and pulse duration. Ensure adequate thermal design - device mounted on TMCS1133xEVM or equivalent PCB design. Isolation barrier rated for 5000V RMS withstand voltage (UL 1577)."
      }
    },
    "IN-": {
      "name": "IN-",
      "pin_id": "2",
      "type": "ANALOG",
      "direction": "INPUT",
      "description": "Input current negative pin. Current flows from IN+ to IN- through an internal low-resistance conductor (0.7mΩ typ at 25°C). Provides return path for sensed current. Maximum continuous current: 80A RMS at 25°C, 44A RMS at 125°C.",
      "implementation": {
        "connectivity": "Connect to low-side current path. Route with heavy copper traces (4oz recommended) matching IN+ design.",
        "components": "Match wire gauge or PCB trace width to IN+ connection.",
        "precautions": "Maintain creepage and clearance ≥8mm between high-voltage input side (pins 1-2) and low-voltage output side (pins 3-10) on PCB. Use appropriate isolation barriers per system safety requirements."
      }
    },
    "GND": {
      "name": "GND",
      "pin_id": "3",
      "type": "POWER",
      "direction": "INPUT",
      "description": "Ground reference for low-voltage circuitry. Signal ground return for internal voltage reference and analog circuits.",
      "implementation": {
        "connectivity": "Connect to system ground plane on low-voltage side. Place decoupling capacitors between VS and GND as close to device pins as possible.",
        "components": "Use low-impedance connection to ground plane.",
        "precautions": "Keep ground path separate from high-current power ground to minimize noise coupling. Maintain isolation barrier clearance from high-voltage input pins."
      }
    },
    "ALERT": {
      "name": "ALERT",
      "pin_id": "4",
      "type": "DIGITAL",
      "direction": "OUTPUT",
      "output_type": "OPEN_DRAIN",
      "polarity": "ACTIVE_LOW",
      "description": "Sensor diagnostics PWM output, open-drain active low. Provides 8kHz PWM signal indicating device status: 100% duty = No Fault, 80% duty = Thermal Alert, 50% duty = Sensor Alert, 20% duty = Thermal and Sensor Alert. Pull-down voltage: 0.07V typ, 0.2V max at 3mA.",
      "output_voltage": {
        "min": 0.0,
        "max": 5.5,
        "units": "V"
      },
      "implementation": {
        "connectivity": "Connect to VS through external pull-up resistor if diagnostics monitoring is needed. Connect to GND if not used.",
        "components": "Pull-up resistor to VS (value depends on desired rise time, typical 4.7kΩ to 10kΩ). Optional low-pass filter for PWM duty cycle measurement.",
        "precautions": "Open-drain output requires external pull-up for logic high state. Maximum sink current should not exceed pull-down voltage specification limits (~3mA recommended)."
      }
    },
    "NC": {
      "name": "NC",
      "pin_id": "5",
      "type": "NC",
      "direction": "NONE",
      "description": "Reserved pin. No internal connection.",
      "implementation": {
        "connectivity": "Can be connected to GND or left floating.",
        "components": "None required.",
        "precautions": "Do not use for routing signals. Reserved for future use."
      }
    },
    "VOUT": {
      "name": "VOUT",
      "pin_id": "6",
      "type": "ANALOG",
      "direction": "OUTPUT",
      "description": "Analog output voltage proportional to input current. VOUT = IIN × S + VOUT,0A where S is device sensitivity (20-150mV/A depending on variant) and VOUT,0A is zero-current reference voltage (2.5V for Axx, 1.65V for Bxx, 0.33V for Cxx variants). Output swing: 5-10mV to VS-20mV (VS-50mV max over temperature). Bandwidth: 1.1MHz (-3dB). Slew rate: 8V/µs. Response time: 120ns. Propagation delay: 50ns. Short circuit output current: 50mA max.",
      "output_voltage": {
        "min": 0.005,
        "max": 5.48,
        "units": "V"
      },
      "implementation": {
        "connectivity": "Connect to ADC input or comparator for current measurement. Route with care to minimize noise pickup.",
        "components": "Optional RC low-pass filter for noise reduction (max capacitive load 4.7nF). Series resistor may be needed for ADC input protection.",
        "precautions": "Do not exceed maximum capacitive load of 4.7nF. Output is short-circuit protected but avoid sustained short conditions. For best accuracy, use within linear output range (100mV to VS-100mV)."
      }
    },
    "OC": {
      "name": "OC",
      "pin_id": "7",
      "type": "DIGITAL",
      "direction": "OUTPUT",
      "output_type": "OPEN_DRAIN",
      "polarity": "ACTIVE_LOW",
      "description": "Overcurrent detection output, open-drain active low. Asserts low when |IIN| exceeds threshold set by VOC pin. Response time: 100ns typ, 250ns max for 120% of IOC step. Threshold error: ±2% typ, ±10% max over temperature. Hysteresis: variant-dependent (1.4A to 8.4A). Pull-down voltage: 0.07V typ, 0.2V max at 3mA.",
      "output_voltage": {
        "min": 0.0,
        "max": 5.5,
        "units": "V"
      },
      "implementation": {
        "connectivity": "Connect to VS through external pull-up resistor for overcurrent detection. Connect to GND if not used.",
        "components": "Pull-up resistor to VS (typical 4.7kΩ to 10kΩ). Connect to MCU interrupt input or gate driver fault input for fast protection response.",
        "precautions": "Open-drain output requires external pull-up. Response time is 100-250ns - faster than analog output processing for overcurrent protection applications."
      }
    },
    "VOC": {
      "name": "VOC",
      "pin_id": "8",
      "type": "ANALOG",
      "direction": "INPUT",
      "description": "Overcurrent threshold setting input. Sets overcurrent detection threshold via formula: VOC = S × IOC / 2.5, where S is sensitivity and IOC is desired overcurrent threshold. Valid range: 0.3V to VS. Input impedance: 120kΩ typ.",
      "input_voltage": {
        "min": 0.3,
        "max": 5.5,
        "units": "V"
      },
      "implementation": {
        "connectivity": "Connect to VS through resistor divider to set fixed threshold, or connect to DAC for programmable threshold. Connect directly to VS if overcurrent detection not used.",
        "components": "Resistor divider from VS (R2 < 10kΩ to minimize input impedance effects). Example: For IOC = 50A with 75mV/A sensitivity, VOC = 0.075 × 50 / 2.5 = 1.5V.",
        "precautions": "Do not leave floating. If OC function not used, connect to VS. Keep R2 of divider <10kΩ for accuracy."
      }
    },
    "VS_9": {
      "name": "VS",
      "pin_id": "9",
      "type": "POWER",
      "direction": "INPUT",
      "description": "Power supply input. Operating range: 3.0V to 5.5V. Quiescent current: 11mA typ at 25°C, 14.5mA max over temperature. Power-on time: 34ms from VS > 3V to valid output.",
      "input_voltage": {
        "min": 3.0,
        "nom": 5.0,
        "max": 5.5,
        "units": "V"
      },
      "implementation": {
        "connectivity": "Connect to regulated 3.0V to 5.5V supply. Both VS pins (9 and 10) must be connected together.",
        "components": "Place 0.1µF low-ESR ceramic decoupling capacitor between VS and GND as close to pins as possible. Additional bulk capacitance (1-10µF) recommended for noisy supplies. Optional ferrite bead in series for high-noise environments.",
        "precautions": "Ensure supply voltage remains within 3.0V to 5.5V range. Supply sequencing is independent of input current - VS can be powered down while high-voltage input is active (within isolation limits). During power-on, output may transition between GND and VS before stabilizing."
      }
    },
    "VS_10": {
      "name": "VS",
      "pin_id": "10",
      "type": "POWER",
      "direction": "INPUT",
      "description": "Power supply input. Operating range: 3.0V to 5.5V. Connect to same supply as pin 9.",
      "input_voltage": {
        "min": 3.0,
        "nom": 5.0,
        "max": 5.5,
        "units": "V"
      },
      "implementation": {
        "connectivity": "Connect to same supply as VS pin 9. Both VS pins must be connected together.",
        "components": "Share decoupling capacitors with pin 9.",
        "precautions": "Do not leave unconnected. Both VS pins must be tied to supply for proper operation."
      }
    }
  },
